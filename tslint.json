{
    "extends": "tslint:recommended",
    "rules": {
        "max-line-length": {
            "options": [120]
        },
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "typedef":false,
        "array-type": false,
        "arrow-parens": false,
        "no-console": false,
        "interface-name": false
    }
}